      "img[src*=\"preview\"]",
      "img[src*=\"redd.it\"]",
      ".upload-success",
      "[class*=\"uploaded\"]",
      "[class*=\"preview\"]",
      "[data-testid*=\"preview\"]",
      "faceplate-carousel",
      "r-post-media-input faceplate-carousel"
    ],
    "upload_completion_indicators": [
      "faceplate-carousel",
      "r-post-media-input faceplate-carousel",
      "#fileInputInnerWrapper faceplate-carousel",
      "div.w-full.relative.flex.rounded-md.overflow-hidden faceplate-carousel"
    ],
    "drop_zones": [
      "#fileInputInnerWrapper",
      "r-post-media-input",
      ".upload-media",
      "[class*=\"drag\"]",
      "[class*=\"drop\"]"
    ],
    "subreddit_selector": [
      "faceplate-search-input[placeholder*=\"community\"]",
      "input[placeholder*=\"Choose a community\"]",
      "faceplate-typeahead-input",
      "[data-testid=\"subreddit-picker-input\"]"
    ],
    "post_type_buttons": {
      "text": [
        "button[data-name=\"TEXT\"]",
        "button:has-text(\"Text\")",
        "[aria-label*=\"Text\"]"
      ],
      "image": [
        "button[data-name=\"IMAGE\"]",
        "button:has-text(\"Images & Video\")",
        "[aria-label*=\"Images\"]"
      ],
      "link": [
        "button[data-name=\"LINK\"]",
        "button:has-text(\"Link\")",
        "[aria-label*=\"Link\"]"
      ]
    }
  },
  "timeouts": {
    "page_load": 30000,
    "element_wait": 10000,
    "file_chooser_wait": 5000,
    "upload_completion": 15000,
    "submit_wait": 10000
  },
  "delays": {
    "typing_min": 0.1,
    "typing_max": 0.3,
    "scroll_min": 2,
    "scroll_max": 8,
    "random_min": 1,
    "random_max": 3,
    "post_upload_min": 3,
    "post_upload_max": 8,
    "pre_submit_min": 2,
    "pre_submit_max": 4,
    "between_uploads_min": 1,
    "between_uploads_max": 3,
    "post_completion_min": 4,
    "post_completion_max": 6,
    "hover_reveal_min": 0.5,
    "hover_reveal_max": 1.5
  }
}