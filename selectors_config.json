      "r-post-media-input #fileInputInnerWrapper",
      "r-post-media-input faceplate-carousel",
      "div.flex.w-full.border-sm.rounded-md.overflow-hidden.group"
    ],
    "file_inputs": [
      "input[type=\"file\"][accept*=\"image\"]",
      "input[data-media-type=\"image\"]",
