      "input[type=\"file\"]",
      "input[accept*=\"image\"]",
      "input[accept*=\"video\"]",
      "input[multiple]"
    ],
    "nsfw_checkbox": [
      "input[name=\"nsfw\"]",
      "faceplate-checkbox[name=\"nsfw\"] input",
      "input[type=\"checkbox\"][id*=\"nsfw\"]",
      "faceplate-switch-input[name=\"isNsfw\"]",
      "faceplate-switch-input[name=\"nsfw\"]"
    ],
