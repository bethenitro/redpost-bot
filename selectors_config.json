      "div[aria-label=\"Post body text field\"][contenteditable=\"true\"][role=\"textbox\"]",
      "shreddit-composer[name=\"body\"] div[contenteditable=\"true\"]",
      "reddit-rte div[contenteditable=\"true\"][role=\"textbox\"]",
      "div[data-lexical-editor=\"true\"][contenteditable=\"true\"]",
      "div[aria-placeholder*=\"Body text\"][contenteditable=\"true\"]",
      "div[data-testid=\"rtjson-text-editor\"]",
      ".public-DraftEditor-content",
      "textarea[name=\"text\"]",
      ".notranslate.public-DraftEditor-content",
      "[contenteditable=\"true\"][role=\"textbox\"]"
    ],
    "upload_buttons": [
      "#device-upload-button",
      "button.upload-media",
      "button:has-text(\"Upload\")",
      "button[class*=\"upload\"]",
      "r-post-media-input button"
    ],
    "add_more_images_buttons": [
      "faceplate-toolbar-action[noun=\"add_more_images\"] button",
      "button.add-media",
      "button:has-text(\"Add images\")",
      "button[class*=\"add-media\"]",
      "faceplate-toolbar-action button[class*=\"add-media\"]",
      "button.add-media.action.invisible.group-hover\\:visible",
      "button[class*=\"group-hover:visible\"]",
      "div.absolute button[class*=\"add-media\"]"
    ],
    "image_hover_areas": [
      "#fileInputInnerWrapper",
      "div.group",
      "faceplate-carousel",
      "div[class*=\"group\"]",
