      "button:has-text(\"Apply\")",
      "button:has-text(\"Save\")",
      "button:has-text(\"Done\")"
    ],
    "spoiler_checkbox": [
      "input[name=\"spoiler\"]",
      "faceplate-checkbox[name=\"spoiler\"] input",
      "input[type=\"checkbox\"][id*=\"spoiler\"]"
    ],
    "submit_buttons": [
      "button:has-text(\"Post\")",
      "button.submit",
      "faceplate-button[type=\"submit\"]",
      "button[class*=\"submit\"]"
    ],
